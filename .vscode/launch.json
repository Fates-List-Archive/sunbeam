{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Compile",
      "request": "launch",
      "runtimeArgs": ["start"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Debug",
      "request": "launch",
      "runtimeArgs": ["run", "preview"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Format",
      "request": "launch",
      "runtimeArgs": ["run", "format"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
