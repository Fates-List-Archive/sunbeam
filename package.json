{
  "name": "sunbeam",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 5000",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview --port 5000",
    "asset-prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile ~/FatesList/data/static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile ~/FatesList/data/static/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify/svelte": "^2.1.0",
    "@smui-extra/accordion": "^6.0.0-beta.11",
    "@smui/button": "^6.0.0-beta.11",
    "@smui/icon-button": "^6.0.0-beta.11",
    "@smui/list": "^6.0.0-beta.11",
    "@smui/menu": "^6.0.0-beta.11",
    "@sveltejs/adapter-auto": "^1.0.0-next.15",
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.8",
    "@sveltejs/kit": "^1.0.0-next.241",
    "@types/cookie": "^0.4.1",
    "@types/lozad": "^1.16.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@zerodevx/svelte-img": "^0.1.0-rc1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "smui-theme": "^6.0.0-beta.11",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "@constemi/itsdangerjs": "github:constemi/itsdangerjs",
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.67",
    "cookie": "^0.4.1",
    "itsdangerous": "^0.0.1",
    "jwt-decode": "^3.1.2",
    "lozad": "^1.16.0",
    "marked": "^4.0.9",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "reallydangerous": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.45.1",
    "svelte-lazy-image": "^0.2.0"
  }
}
